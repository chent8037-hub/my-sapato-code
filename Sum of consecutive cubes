#include <stdio.h>
#include <math.h>

int main() {
    long long n, a;
    printf("這是連續立方和計算機\n");
    printf("輸入開始值\n");
    scanf("%lld", &a);
    printf("輸入結束值\n");
    scanf("%lld", &n);
    long long b=a-1;
    long long ban=(n*(n+1)/2)*(n*(n+1)/2);
    long long z=b*(a)/2;
    long long z2=z*z;
    long long result=ban-z2;
    printf("從%lld的立方到%lld的立方和為%lld\n", a, n, result);
    return 0;
}
