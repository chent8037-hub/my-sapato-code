#include <stdio.h>
#include <stdlib.h>
#include <math.h>
//原公式設函數

int ten_to_two(int input) {
    int out=0;
    int i=0;
    for(i=0; input>0; i++) {
        int bit=input%10;

        if(bit & 1) {
            out +=(int)pow(2, i);
        }
        input /=10;
    }
    return out; 
}


int main() {
    char a[64];
    printf("輸入二進位數字\n");
    scanf(" %s", &a);
    
    long cre=strtol(a, NULL, 2);
    printf("十進位是%ld\n", cre);
    
    return 0;
}
